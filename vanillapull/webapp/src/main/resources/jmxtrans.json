{
    "outputWriters": [
        {
            "@class": "org.jmxtrans.embedded.output.GraphiteWriter",
            "settings": {
                "host": "localhost",
                "port": "2003",
                "enabled": "true",
                "namePrefix": "vanillapull"
            }
        },
        {
                "@class" : "com.octo.vanillapull.monitoring.writers.ThreadCountJsonWriter",
                "settings" : {
                  "outputFile" : "/home/vagrant/outputwritertotrash.log",
                  "maxLogFileSize" : "10MB",
                  "maxLogBackupFiles" : 200,
                  "debug" : true,
                  "typeNames" : ["name"]
                }
        }
    ],
      "queryIntervalInSeconds": 2,
      "numQueryThreads": 1,
      "exportIntervalInSeconds": 2
}
